# -*- coding: utf-8 -*-
# UTF-8 encoding when using korean


for N in range(5):
	seven = input()
	a = 0
	for i in range(len(seven)):
		# if i == 0:
		# 	#a += int(seven[i])
		if i%2 == 0 :
			#print(int(seven[i]))
			a += int(seven[i])

	for i in range(len(seven)):
		if i%2 == 1 and int(seven[i]) != 0:
			a *= int(seven[i])

	a %= 10
	print(a)
